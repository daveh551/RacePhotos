<div class="container-fluid">
	<div class="row-fluid">
		<div class="span12">
			<h3>
				@ViewBag.RaceTitle
			</h3>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span8">
			<div class="carousel slide" id="carousel-513384">
				<ol class="carousel-indicators">
					<li class="active" data-slide-to="0" data-target="#carousel-513384">
					</li>
					<li data-slide-to="1" data-target="#carousel-513384">
					</li>
					<li data-slide-to="2" data-target="#carousel-513384">
					</li>
				</ol>
				<div class="carousel-inner">
					<div class="item active">
						<img alt="" src="http://lorempixel.com/1600/500/sports/1" />
						<div class="carousel-caption">
						
						</div>
					</div>
					<div class="item">
						<img alt="" src="http://lorempixel.com/1600/500/sports/2" />
						<div class="carousel-caption">
						
						</div>
					</div>
					<div class="item">
						<img alt="" src="http://lorempixel.com/1600/500/sports/3" />
						<div class="carousel-caption">
						
						</div>
					</div>
				</div> <a data-slide="prev" href="#carousel-513384" class="left carousel-control">‹</a> <a data-slide="next" href="#carousel-513384" class="right carousel-control">›</a>
			</div> <span class="label">Label</span>
			<div class="btn-group">
				<button class="btn" type="button"><em class="icon-star"></em></button> 
				<button class="btn" type="button"><em class="icon-shopping-cart"></em></button> 
			</div>
		</div>
		<div class="span4">
			<div class="btn-group btn-group-vertical">
				<button class="btn" type="button"><em class="icon-align-left"></em></button> 
				<button class="btn" type="button"><em class="icon-align-center"></em></button> 
				<button class="btn" type="button"><em class="icon-align-right"></em></button> 
				<button class="btn" type="button"><em class="icon-align-justify"></em></button>
			</div>
		</div>
	</div>
</div>
<section class="scripts">
	<script>
		$('.carousel').carousel({ interval: 2000 });
	</script>
</section>